{
    "configurations": [
        {
            "name": "gcc-arm",
            "includePath": [
                "${workspaceFolder}/code/**"
            ],
            "defines": [
                "DEBUG",
                "STM32H743xx",
                "USE_LOGGER=LOGGER_ON",
                "USE_FULL_LL_DRIVER",
                "HSE_VALUE=12000000",
                "HSE_STARTUP_TIMEOUT=100",
                "LSE_STARTUP_TIMEOUT=5000",
                "LSE_VALUE=32768",
                "EXTERNAL_CLOCK_VALUE=12288000",
                "HSI_VALUE=64000000",
                "LSI_VALUE=32000",
                "VDD_VALUE=3300"
            ],
            "cStandard": "gnu23",
            "compilerPath": "${env:GCC_PATH}/arm-none-eabi-gcc.exe -std=c2x",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}